<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.ActorToStringConverter"/>
		<bean class="converters.StringToActorConverter"/>
		
		<bean class="converters.AdministratorToStringConverter"/>
		<bean class="converters.StringToAdministratorConverter"/>
		
		<bean class="converters.ChirpToStringConverter"/>
		<bean class="converters.StringToChirpConverter"/>
		
		<bean class="converters.CommentToStringConverter"/>
		<bean class="converters.StringToCommentConverter"/>
		
		<bean class="converters.DomainEntityToStringConverter"/>
		<bean class="converters.StringToDomainEntityConverter"/>
		
		<bean class="converters.FollowerToStringConverter"/>
		<bean class="converters.StringToFollowerConverter"/>
		
		<bean class="converters.HikeToStringConverter"/>
		<bean class="converters.StringToHikeConverter"/>
		
		<bean class="converters.InnToStringConverter"/>
		<bean class="converters.StringToInnConverter"/>
		
		<bean class="converters.InnkeeperToStringConverter"/>
		<bean class="converters.StringToInnkeeperConverter"/>
		
		<bean class="converters.AuthorityToStringConverter"/>
		<bean class="converters.StringToAuthorityConverter"/>
		
		<bean class="converters.UserToStringConverter"/>
		<bean class="converters.StringToUserConverter"/>
		
		<bean class="converters.AgentToStringConverter"/>
		<bean class="converters.StringToAgentConverter"/>
		
		<bean class="converters.RouteToStringConverter"/>
		<bean class="converters.StringToRouteConverter"/>
		
		<bean class="converters.AdvertisementToStringConverter"/>
		<bean class="converters.StringToAdvertisementConverter"/>
		
		<bean class="converters.ReportToStringConverter"/>
		<bean class="converters.StringToReportConverter"/>

		
	</util:list>
	
</beans>
